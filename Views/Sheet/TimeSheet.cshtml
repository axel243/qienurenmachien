
@model List<DateTime>
@{List<TimeSheet> sheets = ViewBag.Test;
}


<div>
    <table>
        <tr>
            <th>Datum</th>
            <th>Opdracht</th>
            <th>Overwerk</th>
            <th>Verlof</th>
            <th>Ziek</th>
            <th>Training</th>
            <th>Overig</th>
            <th>Verklaring m.b.t. overig</th>
        </tr>


        @{ var sheet = sheets[0].Data;
            var dataDict = Newtonsoft.Json.JsonConvert.DeserializeObject<Dictionary<string, TimeSheet>>(sheet);

        }



        @foreach (var day in dataDict)
        {
        <tr>
            <td>x</td>                         @*<td>@d.Date.ToString("dd/MM/yyyy")</td>*@
            <td>@day.Value.Project</td>
            <td>@day.Value.Overwork</td>
            <td>@day.Value.Absence</td>
            <td>@day.Value.Sick</td>
            <td>@day.Value.Training</td>
            <td>@day.Value.Other</td>
            <td>@day.Value.Data</td>
        </tr>
        }

